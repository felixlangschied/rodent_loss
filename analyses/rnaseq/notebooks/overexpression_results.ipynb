    "import pandas as pd\n",
    "\n",
    "ORGANISMS = [\"human\", \"mouse\"]\n",
    "MIRNAS = [\"mir197\", \"mir769\"]\n",
    "DATA = '../data/overexpression.txt'\n",
    "\n",
    "data = pd.read_csv(DATA, sep='\\t')\n",
    "data.transcript = data.mirna + '-' + data.transcript\n",
    "data['condition'] = data.Organism + '|' + data.transcript\n",
    "display(data)"
   ]
  },
