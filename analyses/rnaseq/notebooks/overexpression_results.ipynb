   "source": [
    "from scipy.stats import ttest_ind\n",
    "\n",
    "def test_significance(df, level=0.05):\n",
    "    col = []\n",
    "    for condition in df.condition.unique():\n",
    "        cdf = df[df.condition == condition]\n",
    "        for mature_mirna in ['197-3p', '769-5p']:\n",
    "            expression = cdf[mature_mirna]\n",
    "            ttest_result = ttest_ind(cdf.CTL, expression)\n",
    "            \n",
    "            t_sig = True if ttest_result.pvalue <= level else False\n",
    "            col.append([condition, mature_mirna, ttest_result.pvalue, t_sig])\n",
    "\n",
    "    pdf = pd.DataFrame(col, columns=['condition', 'mature_mirna', 'ttest', 'ttest_siq'])\n",
    "    # print only hits below significance level\n",
    "    display(pdf[pdf.ttest_siq])\n",
    "    \n",
    "test_significance(data)\n"
   ]
  },
